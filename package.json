{
  "name": "@kurgm/kage-engine",
  "version": "0.5.0",
  "description": "An engine that generates glyph image file from KAGE data",
  "directories": {
    "doc": "docs"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "tsc",
    "build:dist": "mkdir -p dist && rollup -c",
    "build:doc": "typedoc",
    "clean": "rm -r lib dist",
    "lint": "eslint 'src/**/*.ts'",
    "test": "node test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurgm/kage-engine.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kurgm/kage-engine/issues"
  },
  "homepage": "https://github.com/kurgm/kage-engine#readme",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^12.1.1",
    "@stylistic/eslint-plugin": "^2.9.0",
    "eslint": "^9.13.0",
    "rollup": "^4.24.0",
    "tslib": "^2.6.2",
    "typedoc": "^0.26.10",
    "typedoc-plugin-markdown": "^4.2.9",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.11.0"
  }
}
